{
	"compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "noUnusedLocals": false,
    "removeComments": true,
    "module": "commonjs",
    "skipLibCheck": true,
    "strict": true,
    "strictNullChecks": true,
		"target": "es2019",
		"rootDir": "src",
		"outDir": "dist",
		"declaration": true
	},
	"exclude": [
		"node_modules",
    "dist",
    "src/__test__"
	]
}
